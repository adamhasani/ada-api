{
  "name": "Ada API UI",
  "version": "v1.0.0",
  "description": "Simple and easy to use API console for Ada API.",
  "bannerImage": "/src/banner.gif",
  "header": {
    "status": "Online!"
  },
  "apiSettings": {
    "creator": "Ada",
    "apikey": ["ada-api"]
  },
  "categories": [
    {
      "name": "AI (Artificial Intelligence)",
      "items": [
        {
          "name": "LuminAI",
          "desc": "Talk with LuminAI. Contoh: text=halo dari web",
          "path": "/ai/luminai?text=halo%20dari%20web",
          "status": "ready",
          "method": "GET",
          "params": {
            "text": "Text for luminai to respond to"
          }
        },
        {
          "name": "HydroMind",
          "desc": "Chat AI via HydroMind. Contoh: text=halo; model=isi sesuai daftar di mind.hydrooo.web.id",
          "path": "/ai/hydromind?text=halo%20dari%20web&model=gpt-4o",
          "status": "ready",
          "method": "GET",
          "params": {
            "text": "Teks atau perintah untuk chat AI",
            "model": "See the list of supported AI models here: https://mind.hydrooo.web.id"
          }
        }
      ]
    },
    {
      "name": "Downloader",
      "items": [
        {
          "name": "YouTube MP3",
          "desc": "Download Audio YouTube via Link.",
          "path": "/api/download/ytmp3?url=https://youtu.be/na8A4-fapww?si=sFu7VY0yzsUyLeTg",
          "status": "ready",
          "method": "GET",
          "params": {
            "url": "Link YouTube (Contoh: https://youtu.be/...)"
          }
        }
      ]
    },
    {
      "name": "Random",
      "items": [
        {
          "name": "Blue Archive",
          "desc": "Blue Archive random images.",
          "path": "/random/ba",
          "status": "ready",
          "method": "GET"
        }
      ]
    },
    {
      "name": "Search Tools",
      "items": [
        {
          "name": "YouTube",
          "desc": "Video search. Contoh: q=lofi hip hop",
          "path": "/search/youtube?q=lofi%20hip%20hop",
          "status": "ready",
          "method": "GET",
          "params": {
            "q": "Search query"
          }
        }
      ]
    }
  ]
}
